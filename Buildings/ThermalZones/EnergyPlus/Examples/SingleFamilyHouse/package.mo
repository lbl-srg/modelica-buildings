within Buildings.ThermalZones.EnergyPlus.Examples;
package SingleFamilyHouse
  "Package with examples that illustrate the use of the EnergyPlus objects"
  extends Modelica.Icons.ExamplesPackage;
  annotation (
    preferredView="info",
    Documentation(
      info="<html>
<p>
This package contains example models that illustrate the use of the EnergyPlus objects.
All example models use an EnergyPlus model for a single family house.
The house has one conditioned zone, an unconditioned living room, and a garage.
</p>
</html>"));
end SingleFamilyHouse;
