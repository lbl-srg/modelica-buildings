                "equation_section": {
                  "equation": [
                    {
                      "assignment_equation": {
                        "lhs": "T",
                        "rhs": {
                          "simple_expression": {
                            "function_call": {
                              "name": "Medium.temperature",
                              "arguments": [
                                {
                                  "name": "state=Medium.setState_phX(p=port.p,h=inStream(port.h_outflow),X=inStream(port.Xi_outflow))"
