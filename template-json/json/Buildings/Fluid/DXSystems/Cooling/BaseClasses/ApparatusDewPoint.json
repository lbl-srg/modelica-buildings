                            "function_call": {
                              "name": "Medium.specificEnthalpy_pTX",
                              "arguments": [
                                {
                                  "name": "p=Medium.p_default,T=TSatMin,X=cat(1,{XSatMin},{1 -sum({XSatMin})})"
