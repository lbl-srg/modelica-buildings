                        {
                          "declaration": {
                            "identifier": "h_TEvaIn_XADP"
                          },
                          "description": {
                            "description_string": "Enthalpy at inlet air temperature and humidity mass fraction at ADP"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
