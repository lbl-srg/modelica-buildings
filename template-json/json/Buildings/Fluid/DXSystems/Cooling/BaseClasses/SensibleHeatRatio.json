                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "entRat"
                          },
                          "description": {
                            "description_string": "Enthalpy ratio"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
                      "type_prefix": "parameter",
                      "type_specifier": "Modelica.Units.SI.SpecificEnthalpy",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "epsH",
                            "modification": {
                              "equal": true,
                              "expression": {
                                "simple_expression": "100"
                              }
                            }
                          },
                          "description": {
                            "description_string": "Small value for enthalpy to avoid division by zero"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "equation_section": {
                  "equation": [
                    {
                      "if_equation": {
                        "if_elseif": [
                          {
                            "condition": {
                              "simple_expression": "on"
                            },
                            "then": [
                              {
                                "equation": {
                                  "assignment_equation": {
                                    "lhs": "h_TEvaIn_XADP",
                                    "rhs": {
                                      "simple_expression": {
                                        "function_call": {
                                          "name": "Medium.specificEnthalpy_pTX",
                                          "arguments": [
                                            {
                                              "name": "p=p,T=TEvaIn,X=cat(1,{XADP},{1 -XADP})"
                                            }
                                          ]
                                        }
                                      }
                                    }
                                  }
                                }
                              },
                              {
                                "equation": {
                                  "assignment_equation": {
                                    "lhs": "entRat",
                                    "rhs": {
                                      "simple_expression": "(h_TEvaIn_XADP -hADP)/Buildings.Utilities.Math.Functions.smoothMax(x1=epsH,x2=hEvaIn -hADP,deltaX=0.1*epsH)"
                                    }
                                  }
                                }
                              },
                              {
                                "equation": {
                                  "assignment_equation": {
                                    "lhs": "SHR",
                                    "rhs": {
                                      "simple_expression": {
                                        "function_call": {
                                          "name": "Buildings.Utilities.Math.Functions.smoothMin",
                                          "arguments": [
                                            {
                                              "name": "x1=entRat,x2=0.999,deltaX=0.0001"
                                            }
                                          ]
                                        }
                                      }
                                    }
                                  },
                                  "description": {
                                    "description_string": "To restrict the value of SHR in case of zero mass flow rate or dry coil condition"
                                  }
                                }
                              }
                            ]
                          }
                        ],
                        "else_equation": [
                          {
                            "assignment_equation": {
                              "lhs": "h_TEvaIn_XADP",
                              "rhs": {
                                "simple_expression": "0"
                              }
                            }
                          },
                          {
                            "assignment_equation": {
                              "lhs": "entRat",
                              "rhs": {
                                "simple_expression": "1"
                              }
                            }
                          },
                          {
                            "assignment_equation": {
                              "lhs": "SHR",
                              "rhs": {
                                "simple_expression": "0"
                              }
                            }
                          }
                        ]
                      }
                    }
                  ]
