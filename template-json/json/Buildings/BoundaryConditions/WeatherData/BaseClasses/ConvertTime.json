                    "component_clause": {
                      "type_prefix": "constant",
                      "type_specifier": "Modelica.Units.SI.Time",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "shiftSolarRad",
                            "modification": {
                              "equal": true,
                              "expression": {
                                "simple_expression": "1800"
                              }
                            }
                          },
                          "description": {
                            "description_string": "Number of seconds for the shift for solar radiation calculation"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "equation_section": {
                  "equation": [
                    {
                      "assignment_equation": {
                        "lhs": "calTim",
                        "rhs": {
                          "simple_expression": "calTimAux"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "modTimAux",
                        "rhs": {
                          "simple_expression": "modTim"
                        }
                      }
                    },
                    {
                      "function_call_equation": {
                        "function_name": "assert",
                        "function_call_args": {
                          "function_argument": {
                            "expression": {
                              "simple_expression": "canRepeatWeatherFile or noEvent((time -weaDatEndTim) < shiftSolarRad)"
                            }
                          },
                          "function_arguments": {
                            "function_argument": {
                              "expression": {
                                "simple_expression": "\"In \" +getInstanceName() +\": Insufficient weather data provided for the desired simulation period.\n    The simulation time \" +String(time) +\" exceeds the end time \" +String(weaDatEndTim) +\" of the weather data file.\""
                              }
                            },
                            "function_arguments": {
                              "function_argument": {
                                "expression": {
                                  "simple_expression": "AssertionLevel.error"
                                }
                              }
                            }
                          }
                        }
                      }
                    },
                    {
                      "function_call_equation": {
                        "function_name": "assert",
                        "function_call_args": {
                          "function_argument": {
                            "expression": {
                              "simple_expression": "canRepeatWeatherFile or noEvent(time >= weaDatStaTim)"
                            }
                          },
                          "function_arguments": {
                            "function_argument": {
                              "expression": {
                                "simple_expression": "\"In \" +getInstanceName() +\": Insufficient weather data provided for the desired simulation period.\n    The simulation time \" +String(time) +\" is less than the start time \" +String(weaDatStaTim) +\" of the weather data file.\""
                              }
                            },
                            "function_arguments": {
                              "function_argument": {
                                "expression": {
                                  "simple_expression": "AssertionLevel.error"
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              }
