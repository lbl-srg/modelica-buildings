              {
                "equation_section": {
                  "equation": [
                    {
                      "assignment_equation": {
                        "lhs": "modTimAux",
                        "rhs": {
                          "simple_expression": "nDay"
                        }
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "Bt",
                        "rhs": {
                          "simple_expression": "Modelica.Constants.pi*((calTimAux +86400)/86400 -81)/182"
                        }
                      },
                      "description": {
                        "description_string": "Our unit is s instead of day in (A.4.2b)"
                      }
                    },
                    {
                      "assignment_equation": {
                        "lhs": "eqnTim",
                        "rhs": {
                          "simple_expression": "60*(9.87*Modelica.Math.sin(2*Bt) -7.53*Modelica.Math.cos(Bt) -1.5*Modelica.Math.sin(Bt))"
                        }
                      },
                      "description": {
                        "description_string": "Our unit is s instead of min in (A.4.2a)"
                      }
                    }
                  ]
                }
              }
