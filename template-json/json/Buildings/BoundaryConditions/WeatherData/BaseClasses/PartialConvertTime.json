{
  "within": "Buildings.BoundaryConditions.WeatherData.BaseClasses",
  "class_definition": [
    {
      "class_prefixes": "partial block",
      "class_specifier": {
        "long_class_specifier": {
          "identifier": "PartialConvertTime",
          "description_string": "Converts the simulation time to calendar time in scale of 1 year (365 days), or a multiple of a year",
          "composition": {
            "element_list": [
              {
                "extends_clause": {
                  "name": "Modelica.Blocks.Icons.Block"
                }
              },
              {
                "component_clause": {
                  "type_prefix": "parameter",
                  "type_specifier": "Modelica.Units.SI.Time",
                  "component_list": [
                    {
                      "declaration": {
                        "identifier": "weaDatStaTim",
                        "modification": {
                          "class_modification": [
                            {
                              "element_modification_or_replaceable": {
                                "element_modification": {
                                  "name": "displayUnit",
                                  "modification": {
                                    "equal": true,
                                    "expression": {
                                      "simple_expression": "\"d\""
                                    }
                                  }
                                }
                              }
                            }
                          ],
                          "equal": true,
                          "expression": {
                            "simple_expression": "0"
                          }
                        }
                      },
                      "description": {
                        "description_string": "Start time of weather data"
                      }
                    }
                  ]
                }
              },
              {
                "component_clause": {
                  "type_prefix": "parameter",
                  "type_specifier": "Modelica.Units.SI.Time",
                  "component_list": [
                    {
                      "declaration": {
                        "identifier": "weaDatEndTim",
                        "modification": {
                          "class_modification": [
                            {
                              "element_modification_or_replaceable": {
                                "element_modification": {
                                  "name": "displayUnit",
                                  "modification": {
                                    "equal": true,
                                    "expression": {
                                      "simple_expression": "\"d\""
                                    }
                                  }
                                }
                              }
                            }
                          ],
                          "equal": true,
                          "expression": {
                            "simple_expression": "31536000"
                          }
                        }
                      },
                      "description": {
                        "description_string": "End time of weather data"
                      }
                    }
                  ]
                }
              },
              {
                "component_clause": {
                  "type_specifier": "Modelica.Units.SI.Time",
                  "component_list": [
                    {
                      "declaration": {
                        "identifier": "modTimAux"
                      },
                      "description": {
                        "description_string": "Model time"
                      }
                    }
                  ]
                }
              },
              {
                "component_clause": {
                  "type_specifier": "Modelica.Units.SI.Time",
                  "component_list": [
                    {
                      "declaration": {
                        "identifier": "calTimAux"
                      },
                      "description": {
                        "description_string": "Calendar time"
                      }
                    }
                  ]
                }
              }
            ],
            "element_sections": [
              {
                "protected_element_list": [
                  {
                    "component_clause": {
                      "type_prefix": "parameter",
                      "type_specifier": "Modelica.Units.SI.Time",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "lenWea",
                            "modification": {
                              "equal": true,
                              "expression": {
                                "simple_expression": "weaDatEndTim -weaDatStaTim"
                              }
                            }
                          },
                          "description": {
                            "description_string": "Length of weather data"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
                      "type_prefix": "parameter",
                      "type_specifier": "Boolean",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "canRepeatWeatherFile",
                            "modification": {
                              "equal": true,
                              "expression": {
                                "simple_expression": {
                                  "logical_expression": {
                                    "logical_or": [
                                      {
                                        "logical_and": [
                                          {
                                            "arithmetic_expressions": [
                                              {
                                                "name": "abs(mod(lenWea,365*24*3600))"
                                              },
                                              {
                                                "name": "0.01"
                                              }
                                            ],
                                            "relation_operator": "<"
                                          }
                                        ]
                                      }
                                    ]
                                  }
                                }
                              }
                            }
                          },
                          "description": {
                            "description_string": "=true, if the weather file can be repeated, since it has the length of a year or a multiple of it"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component_clause": {
                      "type_prefix": "discrete",
                      "type_specifier": "Modelica.Units.SI.Time",
                      "component_list": [
                        {
                          "declaration": {
                            "identifier": "tNext",
                            "modification": {
                              "class_modification": [
                                {
                                  "element_modification_or_replaceable": {
                                    "element_modification": {
                                      "name": "start",
                                      "modification": {
                                        "equal": true,
                                        "expression": {
                                          "simple_expression": "0"
                                        }
                                      }
                                    }
                                  }
                                },
                                {
                                  "element_modification_or_replaceable": {
                                    "element_modification": {
                                      "name": "fixed",
                                      "modification": {
                                        "equal": true,
                                        "expression": {
                                          "simple_expression": "true"
                                        }
                                      }
                                    }
                                  }
                                }
                              ]
                            }
                          },
                          "description": {
                            "description_string": "Start time of next period"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "equation_section": {
                  "equation": [
                    {
                      "when_equation": [
                        {
                          "condition": {
                            "simple_expression": "{initial(),canRepeatWeatherFile and modTimAux > pre(tNext)}"
                          },
                          "then": [
                            {
                              "assignment_equation": {
                                "lhs": "tNext",
                                "rhs": {
                                  "if_expression": {
                                    "if_elseif": [
                                      {
                                        "condition": {
                                          "simple_expression": "canRepeatWeatherFile"
                                        },
                                        "then": {
                                          "simple_expression": "integer(modTimAux/lenWea)*lenWea +lenWea"
                                        }
                                      }
                                    ],
                                    "else_expression": {
                                      "simple_expression": "time"
                                    }
                                  }
                                }
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "assignment_equation": {
                        "lhs": "calTimAux",
                        "rhs": {
                          "if_expression": {
                            "if_elseif": [
                              {
                                "condition": {
                                  "simple_expression": "canRepeatWeatherFile"
                                },
                                "then": {
                                  "simple_expression": "modTimAux -tNext +lenWea"
                                }
                              }
                            ],
                            "else_expression": {
                              "simple_expression": "modTimAux"
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              }
            ],
            "annotation": [
              {
                "element_modification_or_replaceable": {
                  "element_modification": {
                    "name": "defaultComponentName",
                    "modification": {
                      "equal": true,
                      "expression": {
                        "simple_expression": "\"conTim\""
                      }
                    }
                  }
                }
              },
              {
                "element_modification_or_replaceable": {
                  "element_modification": {
                    "name": "Documentation",
                    "modification": {
                      "class_modification": [
                        {
                          "element_modification_or_replaceable": {
                            "element_modification": {
                              "name": "info",
                              "modification": {
                                "equal": true,
                                "expression": {
                                  "simple_expression": "\"<html>\n<p>\nThis component converts the simulation time to calendar time in a scale of 1 year (365 days),\nor a multiple of it, if this is the length of the weather file.\n</p>\n</html>\""
                                }
                              }
                            }
                          }
                        },
                        {
                          "element_modification_or_replaceable": {
                            "element_modification": {
                              "name": "revisions",
                              "modification": {
                                "equal": true,
                                "expression": {
                                  "simple_expression": "\"<html>\n<ul>\n<li>\nMarch 27, 2023, by Ettore Zanetti:<br/>\nAdded partial class for conversion from simulation time to calendar time, to be\nused by solar models that require calendar time for calculations.<br/>\nThis is for <a href=\\\"https://github.com/ibpsa/modelica-ibpsa/issues/1716\\\">IBPSA #1716</a>.\n</li>\n</ul>\n</html>\""
                                }
                              }
                            }
                          }
                        }
                      ]
                    }
                  }
                }
              },
              {
                "element_modification_or_replaceable": {
                  "element_modification": {
                    "name": "Icon",
                    "modification": {
                      "class_modification": [
                        {
                          "element_modification_or_replaceable": {
                            "element_modification": {
                              "coordinateSystem": {
                                "extent": [
                                  {
                                    "x": -100,
                                    "y": -100
                                  },
                                  {
                                    "x": 100,
                                    "y": 100
                                  }
                                ],
                                "preserveAspectRatio": "true"
                              }
                            }
                          }
                        }
                      ]
                    }
                  }
                }
              }
            ]
          }
        }
      }
    }
  ],
